.commit {
  position: relative;
  align-items: flex-start;
  margin-top: 3%;
  padding-bottom: 3%;
  margin-right: 2%;
  border-bottom: 1px solid #f2f2f2;
}

.commit_avatar {
  width: 15%;
}

.commit_avatar image {
  width: 100%;
  display: block;
  border-radius: 10rpx;
}

.commit_avatar+view {
  width: 83%;
  margin-left: 2%;
}

.commit_text {
  align-items: flex-start;
}

.commit_name_date {
  justify-content: space-between;
}

.commit_name_date+view {
  margin-top: 2%;
}

.comment_min_image {
  margin-top: 2%;
}

.comment_min_image image {
  width: 23.5%;
  margin-right: 1.75%;
  justify-content: flex-start;
}

.comment_min_image image:last-child {
  margin-right: 0rpx;
}

.position_content {
  position: absolute;
  width: 100%;
  z-index: 101;
}

.slide-image {
  width: 100%;
  display: block;
  
}
